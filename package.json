{
  "name": "hyper-tunnel",
  "version": "1.1.0",
  "description": "noncloud is HTTPS tunnel over WSS",
  "main": "bin/server.js",
  "scripts": {
    "start": "node bin/server.js -a $USERNAME:$PASSWORD -p $PORT",
    "test:server": "node bin/server.js -a user:pass -p 4000",
    "test:client": "node bin/client.js -a user:pass -l localhost:8000 -r localhost:4000 -p http:ws:http",
    "build": "tsc"
  },
  "bin": {
    "htunnel-client": "bin/client.js",
    "htunnel-server": "bin/server.js"
  },
  "author": "TANIGUCHI Masaya",
  "license": "GPL",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "circular-json": "^0.5.1",
    "commander": "^2.14.1",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "uuid": "^3.2.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.8",
    "@types/circular-json": "^0.4.0",
    "@types/commander": "^2.12.2",
    "@types/compression": "0.0.35",
    "@types/express": "^4.11.1",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
